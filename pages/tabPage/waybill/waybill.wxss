
.search_frame {
  padding: 0 24rpx;
  display: flex;
  height: 96rpx;
  align-items: center;
}
.search_placeholder{
  font-size: 24rpx;
  font-weight: 400;
  color: #C3CBD6;
  padding-left: 24rpx;
  background-repeat: no-repeat;
  background-size: 20rpx 20rpx;
  background-position-y: center;
  background-image: url(data:image/png;base64,) ;
}
.search_input{
  padding-left: 24rpx;
  font-size: 28rpx;
  width: 600rpx;
  height: 64rpx;
  background: #F9F9F9;
  border-radius: 8rpx;
}
.search_frame .title{
  font-size: 28rpx;
  display: block;
  padding-left: 16rpx;
  font-weight: 400;
  color: #7E8BA8;
}
.content{
  padding: 10rpx 20rpx;
  box-sizing: border-box;
  height: 90vh;
}
.content .card{
  position: relative;
  padding-left: 32rpx;
  min-height: 300rpx;
  box-shadow: 0px 0px 20rpx 0 rgba(195, 203, 214, 0.74);
  border-radius: 12rpx;
  margin-bottom: 40rpx;
}
.content .card::after{
  position: absolute;
  left: 0;
  top: 24rpx;
  display: block;
  content: '';
  width: 8rpx;
  height: 32rpx;
  background-color: #1890FF;
}
.card .header-title{
  border-bottom: 2rpx #EEE dashed;
  height: 88rpx;
}
.card .number{
  float: left;
  line-height: 88rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: #515A6E;
}
.card .created{
  float: right;
  line-height: 88rpx;
  font-size: 20rpx;
  font-weight: 500;
  color: #515A6E;
  padding-top: 16rpx;
}
.card .info {
  min-height: 130rpx;
  height: 100%;
  box-sizing: border-box;
  padding: 24rpx 0;
  border-bottom: 2rpx #EEE dashed;
}
.card .info view{
  display: inline-block;
  width: 50%;
  font-size: 24rpx;
  font-weight: 400;
  color: #808695;
  line-height: 40rpx;
}
.card .info .right{
  box-sizing: border-box;
  text-align: right;
  padding-right: 24rpx;
}
.card .status{
  padding: 22rpx 0;
}
.card .status text{
  display: inline-block;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #515A6E;
}
.card .status .line{
  border-right: 2rpx solid #C3CBD6;
}
.card .info .path{
  width: 100%;
  padding-right: 24rpx;
  box-sizing: border-box;
}
